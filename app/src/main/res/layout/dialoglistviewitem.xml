<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="@dimen/float_item_heigt"
    android:orientation="horizontal"
    android:id="@+id/cmd_layout"
    >
    <TextView
        android:layout_width="0dp"
        android:layout_height="@dimen/float_item_heigt"
        android:layout_weight="3"
        android:id="@+id/cmd_name"
        android:gravity="center"
        android:layout_gravity="center_vertical"
        android:textSize="@dimen/text_size_14"
        android:textColor="@color/black"
        />

    <TextView
        android:layout_width="0dp"
        android:layout_height="@dimen/float_item_heigt"
        android:layout_weight="3"
        android:id="@+id/cmd_alias"
        android:gravity="center"
        android:layout_gravity="center_vertical"
        android:textSize="@dimen/text_size_14"
        android:textColor="@color/black"
        />

    <TextView
        android:layout_width="0dp"
        android:layout_height="@dimen/float_item_heigt"
        android:layout_weight="3"
        android:id="@+id/cmd_start"
        android:gravity="center"
        android:textSize="@dimen/text_size_14"
        android:layout_gravity="center_vertical"
        android:textColor="@color/black"
        />
    <View
        android:layout_width="0dp"
        android:layout_height="2dp"
        android:layout_weight="1"
        android:layout_gravity="center_vertical"
        android:background="@color/red"
        android:paddingRight="10dp"
        android:paddingLeft="10dp"
        />
    <TextView
        android:layout_width="0dp"
        android:layout_height="@dimen/float_item_heigt"
        android:layout_weight="3"
        android:id="@+id/cmd_end"
        android:gravity="center"
        android:layout_gravity="center_vertical"
        android:textSize="@dimen/text_size_14"
        android:textColor="@color/black"
        />
    <View
        android:layout_width="0dp"
        android:layout_height="2dp"
        android:layout_weight="1"
        android:layout_gravity="center_vertical"
        android:background="@color/red"
        android:paddingRight="10dp"
        android:paddingLeft="10dp"
        />
    <TextView
        android:layout_width="0dp"
        android:layout_height="@dimen/float_item_heigt"
        android:layout_weight="3"
        android:id="@+id/cmd_read_start"
        android:gravity="center"
        android:layout_gravity="center_vertical"
        android:textSize="@dimen/text_size_14"
        android:textColor="@color/black"
        />
    <View
        android:layout_width="0dp"
        android:layout_height="2dp"
        android:layout_weight="1"
        android:layout_gravity="center_vertical"
        android:background="@color/red"
        android:paddingRight="10dp"
        android:paddingLeft="10dp"
        />
    <TextView
        android:layout_width="0dp"
        android:layout_height="@dimen/float_item_heigt"
        android:layout_weight="3"
        android:id="@+id/cmd_read_middle"
        android:gravity="center"
        android:layout_gravity="center_vertical"
        android:textSize="@dimen/text_size_14"
        android:textColor="@color/black"
        />
    <View
        android:layout_width="0dp"
        android:layout_height="2dp"
        android:layout_weight="1"
        android:layout_gravity="center_vertical"
        android:background="@color/red"
        android:paddingRight="10dp"
        android:paddingLeft="10dp"
        />
    <TextView
        android:layout_width="0dp"
        android:layout_height="@dimen/float_item_heigt"
        android:layout_weight="4"
        android:id="@+id/cmd_read_end"
        android:gravity="center"
        android:layout_gravity="center_vertical"
        android:textSize="@dimen/text_size_14"
        android:textColor="@color/black"
        />
</LinearLayout>